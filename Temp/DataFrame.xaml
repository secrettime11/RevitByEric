<UserControl x:Class="Temp.DataFrame"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Temp"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="200">
    <Grid Background="White">
        <StackPanel x:Name="ImgPanel" Margin="2,2,92,2" MouseUp="ImgPanel_MouseUp"/>
        <StackPanel x:Name="FamilyPanel" Margin="110,2,2,52">
            <Label x:Name="lb_Family" Content="Label" Margin="2,10,2,2" />
        </StackPanel>
        <StackPanel x:Name="SymbolPanel" Margin="110,52,2,2">
            <Label x:Name="lb_Symbol" Content="Label" Margin="2,10,2,2"/>
        </StackPanel>
    </Grid>
</UserControl>
